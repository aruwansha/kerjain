<div class="card mt-2 mb-4">
  <div class="card-body">
      <div class="form-group">
        <label for="inputName">Nama</label>
        <input
          type="text"
          class="form-control input-disabled cursor-default"
          name="name"
          value="<%= freelancer.userId.name %>"
          readonly
          />
      </div>
      <div class="form-group">
        <label for="inputEmail">Email</label>
        <input
          type="text"
          class="form-control input-disabled cursor-default"
          name="email"
          value="<%= freelancer.userId.email %>"
          readonly
          />
      </div>
      <div class="form-group">
        <label for="inputAddress">Alamat</label>
        <input
          type="text"
          class="form-control input-disabled cursor-default"
          name="address"
          value="<%= freelancer.userId.address %>"
          readonly
          />
      </div>
      <div class="form-group">
        <label for="inputPhone">Nomer Telepon</label>
        <input
          type="text"
          class="form-control input-disabled cursor-default"
          name="phone"
          value="<%= freelancer.userId.phone %>"
          readonly
          />
      </div>
      <div class="form-group">
        <label for="inputPhone">Foto Profil</label>
        <br />
        <img src="<%= process.env.URL %><%= freelancer.userId.imgUrl %>" alt="" width="200" height="200" class="img-fluid mb-2" />
      </div>
      <hr />
  </div>
</div>
