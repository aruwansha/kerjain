<%- include('../../partials/header.ejs') %>
<!-- Page Wrapper -->
<div id="wrapper">
  <!-- Sidebar -->
  <%- include('../../partials/admin/sidebar.ejs') %>
  <!-- End of Sidebar -->

  <!-- Content Wrapper -->
  <div id="content-wrapper" class="d-flex flex-column">
    <!-- Main Content -->
    <div id="content">
      <!-- Topbar -->
      <%- include('../../partials/navbar.ejs') %>
      <!-- End of Topbar -->

      <!-- Begin Page Content -->
      <div class="container-fluid">
        <!-- Page Heading -->
        <h1 class="h3 mb-4 text-gray-800">Service User</h1>
        <%- include('../../partials/message.ejs') %> <% if(action == 'view') {
        %> <%- include('show_table_service_user.ejs')%> <% } else if(action ==
        'detail') { %>
        <ul class="nav nav-tabs" id="myTab" role="tablist">
          <li class="nav-item">
            <a
              class="nav-link active"
              id="edit-tab"
              data-toggle="tab"
              href="#edit-service-user"
              role="tab"
              aria-controls="home"
              aria-selected="true"
              >Detail Service User</a
            >
          </li>
          <li class="nav-item">
            <a
              class="nav-link"
              id="delete-tab"
              data-toggle="tab"
              href="#delete-service-user"
              role="tab"
              aria-controls="home"
              aria-selected="true"
              >Danger Zone</a
            >
          </li>
        </ul>
        <div class="tab-content" id="myTabContent">
          <%- include('show_edit_service_user.ejs')%> <%-
          include('show_delete_zone.ejs')%>
        </div>
        <% } %>
      </div>
      <!-- /.container-fluid -->
    </div>
    <!-- End of Main Content -->

    <!-- Footer -->
    <%- include('../../partials/footer.ejs') %>
    <!-- End of Footer -->
  </div>
  <!-- End of Content Wrapper -->
</div>
<!-- End of Page Wrapper -->

<!-- Scroll to Top Button-->
<a class="scroll-to-top rounded" href="#page-top">
  <i class="fas fa-angle-up"></i>
</a>
<%- include('add_modal.ejs') %> <%- include('../../partials/js.ejs') %>
