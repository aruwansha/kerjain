
  <div class="card mt-2 mb-4">
    <div class="card-header py-3">
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table
          class="table table-bordered text-center"
          id="dataTable"
          width="100%"
          cellspacing="0"
        >
          <thead>
            <tr>
              <th>No</th>
              <th>Name</th>
              <th>Email</th>
              <th>Status</th>
              <th>Action</th>
            </tr>
          </thead>

          <tbody>
            <% for(let i = 0; i < serviceUser.length; i++) { %>
            <tr>
              <td><%= i+1 %></td>
              <td><%= serviceUser[i].userId.name %></td>
              <td><%= serviceUser[i].userId.email %></td>
              <td>
                <% if( serviceUser[i].userId.isBanned == false) { %> Active <% } else{
                %> Unactive <% } %>
              </td>

              <td>
                <a
                  href="/admin/serviceuser/<%= serviceUser[i]._id %>"
                  class="btn btn-primary btn-circle btn-sm"
                  ><i class="fas fa-eye"></i
                ></a>
              </td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </div>

